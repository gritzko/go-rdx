test:eq(    "deletion diff"
            "removed"@1
            rdx:diff("removed")
       )

test:eq(    "no diff diff"
            A
            rdx:diff(A A) )

test:eq(    "diff with an update"
            (@4 1@1 2@2)
            rdx:diff((1) (2))
       )

test:eq(    "tuple diff with update/append"
            (@4 1 2 3@2)
            rdx:diff((1 2) (1 2 3))
       )

test:eq(    "set diff"
            {@4 1 2@2 3 4 5@1}
            rdx:diff(
                    {1 3 4 5}
                    {1 2 3 4}
                    )
        )

test:eq(    "array diff"
            [@4 1 2@2 3 4 5@1]
            rdx:diff(
                    [1 3 4 5]
                    [1 2 3 4]
                    )
        )

test:eq(    "map diff"
            {@4 1:"one" (@2 2 "two") 3:"three" (@1 4 "four") (@2 4 "FOUR") (@1 5 "five")}
            rdx:diff(
                    {1:"one" 3:"three" 4:"four" 5:"five"}
                    {1:"one" 2:"two" 3:"three" 4:"FOUR"}
                    )
        )

